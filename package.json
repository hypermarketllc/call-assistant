{
  "name": "call-assistant",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"cross-env NODE_ENV=development node src/server/proxy.js\"",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "webhook": "cross-env NODE_ENV=production node src/server/webhook.js",
    "start": "cross-env NODE_ENV=production pm2 start ecosystem.config.js",
    "stop": "pm2 stop all",
    "restart": "pm2 restart all",
    "logs": "pm2 logs",
    "status": "pm2 status",
    "clean": "pm2 delete all"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "formdata-node": "^5.0.1",
    "lucide-react": "^0.344.0",
    "pm2": "^5.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "undici": "^6.6.2",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/file-saver": "^2.0.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}